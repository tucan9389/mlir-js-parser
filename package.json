{
  "name": "mlir-js-parser",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "find-llvm": "bash scripts/find-llvm.sh",
    "bootstrap:wasm-llvm": "bash scripts/bootstrap-llvm-wasm.sh",
    "build:native": "bash scripts/build-native.sh",
    "build:wasm": "bash scripts/build-wasm.sh",
    "sample:node": "node wasm/sample/node.mjs",
    "serve": "npx http-server wasm/sample",
    "size:report": "node scripts/size-report.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --config vitest.config.mjs --reporter=dot",
    "test:snap": "vitest --config vitest.snap.config.mjs",
    "test:snap:run": "vitest run --config vitest.snap.config.mjs --reporter=dot"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "vitest": "^1.6.0"
  }
}
